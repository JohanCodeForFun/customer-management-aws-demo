{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run --exclude src/services/__tests__/connectionTest.test.ts",
    "test:unit": "vitest run src/services/__tests__/customerService.test.ts",
    "test:unit:ci": "vitest run src/services/__tests__/customerService.test.ts --no-coverage --reporter=verbose",
    "test:integration": "vitest run src/services/__tests__/connectionTest.test.ts",
    "test:ci": "vitest run --reporter=junit --outputFile=test-results.xml --exclude src/services/__tests__/connectionTest.test.ts",
    "test:integration:ci": "vitest run src/services/__tests__/connectionTest.test.ts --reporter=junit --outputFile=integration-test-results.xml"
  },
  "dependencies": {
    "axios": "^1.13.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@types/supertest": "^6.0.3",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/ui": "^4.0.8",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "jsdom": "^27.1.0",
    "supertest": "^7.1.4",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7",
    "vitest": "^4.0.8"
  }
}
